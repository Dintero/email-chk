        "lib"
    ],
    "sideEffects": "false",
    "devDependencies": {
        "@biomejs/biome": "2.3.10",
        "@semantic-release/git": "10.0.1",
        "@types/node": "24.10.3",
        "semantic-release": "25.0.2",
        "ts-node": "10.9.2",
        "typescript": "5.9.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Dintero/email-chk.git"
    },
    "homepage": "https://github.com/Dintero/email-chk",
