{
  "name": "@dintero/email-chk",
  "version": "0.0.0-development",
  "description": "",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "author": "Thoralf Thelle <thoralf@dintero.com>",
  "license": "MIT",
  "private": false,
  "files": [
    "lib"
  ],
  "sideEffects": "false",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.34.0",
    "eslint-config-dintero": "https://github.com/Dintero/eslint-config-dintero",
    "jest": "^29.4.2",
    "semantic-release": "^21.0.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dintero/email-chk"
  },
  "homepage": "https://github.com/Dintero/email-chk",
  "bugs": {
    "url": "https://github.com/Dintero/email-chk/issues"
  },
  "scripts": {
    "test": "jest",
    "clean": "rm -rf ./lib",
    "check": "yarn run lint && yarn run test",
    "build": "yarn run clean && yarn run build:esm && yarn run build:cjs",
    "build:esm": "tsc -p ./tsconfig.esm.json && echo '{ \"type\": \"module\" }' > ./lib/esm/package.json",
    "build:cjs": "tsc -p ./tsconfig.cjs.json && echo '{ \"type\": \"commonjs\" }' > ./lib/cjs/package.json",
    "lint": "eslint ./src",
    "release": "semantic-release",
    "prepublishOnly": "yarn run build"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
